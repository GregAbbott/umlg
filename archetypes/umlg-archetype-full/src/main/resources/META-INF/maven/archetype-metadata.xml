<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
        xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
        name="demo111"
        xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <requiredProperties>
        <requiredProperty key="modelName" />
    </requiredProperties>
    <modules>
        <module id="generator" dir="generator" name="generator">
            <fileSets>
                <fileSet filtered="true" packaged="true" encoding="UTF-8">
                    <directory>src/main/java</directory>
                    <includes>
                        <include>**/*.java</include>
                    </includes>
                </fileSet>
            </fileSets>
        </module>
        <module id="application" dir="application" name="application">
            <fileSets>
                <fileSet filtered="true" encoding="UTF-8">
                    <directory>src/main/resources</directory>
                    <includes>
                        <include>**/*.properties</include>
                    </includes>
                </fileSet>
                <fileSet encoding="UTF-8">
                    <directory>src/main/model</directory>
                    <includes>
                        <include>**/*.di</include>
                        <include>**/*.uml</include>
                        <include>**/*.notation</include>
                    </includes>
                </fileSet>
                <fileSet filtered="true" encoding="UTF-8">
                    <directory>src/main/java</directory>
                </fileSet>
                <fileSet filtered="true" encoding="UTF-8">
                    <directory>src/main/generated-java</directory>
                </fileSet>
                <fileSet filtered="true" encoding="UTF-8">
                    <directory>src/main/generated-java-meta</directory>
                </fileSet>
                <fileSet filtered="true" encoding="UTF-8">
                    <directory>src/main/generated-java-restlet</directory>
                </fileSet>
                <fileSet filtered="true" packaged="false" encoding="UTF-8">
                    <directory>src/test/java</directory>
                    <includes>
                        <include>**/*.java</include>
                    </includes>
                </fileSet>
            </fileSets>
        </module>
    </modules>
</archetype-descriptor>
